<?xml version="1.0" encoding="UTF-8"?>
<FineSDK>
  	<AppName>Fine小米</AppName>
  	<PackageName>com.fine.sdk.xiaomi</PackageName>
  	<ApplicationName>com.fine.sdk.dispatcher.MiApplication</ApplicationName>
  	<ScreenOrientation>landscape</ScreenOrientation>
  	<LaunchMode>singleTask</LaunchMode>
  	<LaunchIntent>
	    <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        <meta-data android:name="android.vendor.home_indicator" android:value="hide" />
	</LaunchIntent>
	<FixedScreenOrientationList></FixedScreenOrientationList>
</FineSDK>
